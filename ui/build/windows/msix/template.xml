<?xml version="1.0" encoding="utf-8"?>
<MsixPackagingToolTemplate
    xmlns="http://schemas.microsoft.com/msix/packaging/msixpackagingtool/template/2022">
    <Settings
        AllowTelemetry="false"
        ApplyACLsToPackageFiles="true"
        GenerateCommandLineFile="true"
        AllowPromptForPassword="false">
    </Settings>
    <Installer
        Path="safechain-ultimate-ui"
        Arguments=""
        InstallLocation="C:\Program Files\Aikido Security\safechain-ultimate-ui">
    </Installer>
    <PackageInformation
        PackageName="safechain-ultimate-ui"
        PackageDisplayName="safechain-ultimate-ui"
        PublisherName="CN=Aikido Security"
        PublisherDisplayName="Aikido Security"
        Version="0.1.0.0"
        PackageDescription="A safechain-ultimate-ui application">
        <Capabilities>
            <Capability Name="runFullTrust" />
            
        </Capabilities>
        <Applications>
            <Application
                Id="com.example.safechainultimateui"
                Description="A safechain-ultimate-ui application"
                DisplayName="safechain-ultimate-ui"
                ExecutableName="safechain-ultimate-ui"
                EntryPoint="Windows.FullTrustApplication">
                
            </Application>
        </Applications>
        <Resources>
            <Resource Language="en-us" />
        </Resources>
        <Dependencies>
            <TargetDeviceFamily Name="Windows.Desktop" MinVersion="10.0.17763.0" MaxVersionTested="10.0.19041.0" />
        </Dependencies>
        <Properties>
            <Framework>false</Framework>
            <DisplayName>safechain-ultimate-ui</DisplayName>
            <PublisherDisplayName>Aikido Security</PublisherDisplayName>
            <Description>A safechain-ultimate-ui application</Description>
            <Logo>Assets\AppIcon.png</Logo>
        </Properties>
    </PackageInformation>
    <SaveLocation PackagePath="safechain-ultimate-ui.msix" />
    <PackageIntegrity>
        <CertificatePath></CertificatePath>
    </PackageIntegrity>
</MsixPackagingToolTemplate>
